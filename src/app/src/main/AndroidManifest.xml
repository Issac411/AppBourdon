<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.issac.ppe2sanskt">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the 'MyLocation' functionality. 
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.issac.myapplication.IHM.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.issac.myapplication.IHM.Importance_management" />
        <activity android:name="com.example.issac.myapplication.IHM.specialisation.Specialisation_add" />
        <activity android:name="com.example.issac.myapplication.IHM.specialisation.Specialisation_vue" />
        <activity android:name="com.example.issac.myapplication.IHM.specialisation.Specialisation_details" />
        <activity android:name="com.example.issac.myapplication.IHM.Commercial_add" />
        <activity android:name="com.example.issac.myapplication.IHM.company.Company_add" />
        <activity android:name="com.example.issac.myapplication.IHM.company.Company_vue" />
        <activity android:name="com.example.issac.myapplication.IHM.company.Company_details" />
        <activity android:name="com.example.issac.myapplication.IHM.company.Company_edit" />
        <activity android:name="com.example.issac.myapplication.IHM.entry.Entry_add" />
        <activity android:name="com.example.issac.myapplication.IHM.company.Company_manage_specialisation"></activity>
    </application>

</manifest>